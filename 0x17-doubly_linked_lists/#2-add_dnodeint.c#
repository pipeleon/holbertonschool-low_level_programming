#include "lists.h"
/* Task 2 0x17. C - Doubly linked lists  */

/**
 * add_dnodeint - adds a new node at the beginning of a dlistint_t list
 * @head: List
 * @n: new int
 *
 * Return: dlistint_t
 */

{
	int i;

	i = 0;
	if (h != NULL)
	{
		if (h->next != NULL)
			i += dlistint_len(h->next);
		i++;
	}

	return (i);
}
